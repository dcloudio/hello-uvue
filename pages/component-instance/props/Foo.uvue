<template>
	<view class="component">
		<text id="foo-arr">arr: {{JSON.stringify(arr)}}</text>
	</view>
</template>

<script>
	export default {
		props: {
			arr: {
				type: Array as PropType<number[]>,
				default: () : Array<number> => {
					return [1, 2, 3]
				}
			}
		}
	}
</script>