<template>
  <view>
    <view class="row">
      <text>hasPropsAttrs</text>
      <text class="has-props-attrs">{{ hasPropsAttrs }}</text>
    </view>
    <view class="row">
      <text>hasEmitsAttr</text>
      <text class="has-emits-attrs">{{ hasEmitsAttr }}</text>
    </view>
    <view class="row">
      <text>hasAttrs</text>
      <text class="has-attrs">{{ hasAttrs }}</text>
    </view>
  </view>
</template>

<script lang='uts'>
export default {
  emits: ['result'],
  props: {
    val: {
      type: String,
      default: "default value"
    }
  },
  computed: {
    hasPropsAttrs(): boolean {
      // #ifdef APP-ANDROID
      return this.$attrs.has('val')
      // #endif
      // #ifndef APP-ANDROID
      return !!this.$attrs['val']
      // #endif
    },
    hasEmitsAttr (): boolean {
      // #ifdef APP-ANDROID
      return this.$attrs.has('result')
      // #endif
      // #ifndef APP-ANDROID
      return !!this.$attrs['result']
      // #endif
    },
    hasAttrs (): boolean {
      // #ifdef APP-ANDROID
      return this.$attrs.has('class')
      // #endif
      // #ifndef APP-ANDROID
      return !!this.$attrs['class']
      // #endif
    }
  }
}
</script>

<style scoped>
.row {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-bottom: 10px;
}
</style>
