<template>
  <view class="page">
    <text id="count" class="mb-10">count: {{ count }}</text>
    <text id="obj-str" class="mb-10">obj.str: {{obj['str']}}</text>
    <text id="obj-num" class="mb-10">obj.num: {{obj['num']}}</text>
    <text id="obj-arr" class="mb-10">obj.arr: {{JSON.stringify(obj['arr'])}}</text>
    <button class="update-btn" @click="update">update</button>
  </view>
</template>

<script setup>
  const count = ref(0)

  const obj = reactive({
    str: 'default str',
    num: count,
    arr: ['a', 'b', 'c']
  })

  const update = () => {
    obj['str'] = 'new str';
    obj['num'] = (obj['num'] as number) + 1
    count.value++
    (obj['arr'] as string[]).push('d')
  }
</script>